  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/node": "^20.11.0",
    "@types/multer": "^2.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.7",
    "@types/passport-google-oauth20": "^2.0.14",
    "dotenv-cli": "^7.4.0",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
