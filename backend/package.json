    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.3.2",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "passport-steam": "^1.0.18",
    "redis": "^4.6.12",
    "socket.io": "^4.6.1"
